pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scmGit(
                    branches: [[name: '**']], 
                    extensions: [], 
                    userRemoteConfigs: [[
                        url: 'https://github.com/GeorgesGil/app-semana-05.git'
                        ]])
          
            }
        }
       
	   
	   
	   
	   
    }
}